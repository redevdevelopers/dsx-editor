{
  "name": "dsx-editor",
  "version": "1.0.0",
  "description": "A rhythm game chart editor.",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jest": "^30.2.0"
  },
  "devDependencies": {
    "@types/pixi.js": "^5.0.0",
    "electron": "^31.2.1",
    "electron-builder": "^25.0.0"
  },
  "build": {
    "appId": "com.dreamsyncx.editor",
    "productName": "DreamSyncX Chart Editor",
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}"
    ],
    "win": {
      "target": [
        "portable"
      ],
      "icon": "assets/icon/dsxicon256.ico",
      "forceCodeSigning": false,
      "signAndEditExecutable": false
    }
  }
}
